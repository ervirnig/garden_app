    <th>Latin name</th>
    <th>Common name</th>
    <th>Bed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @flower_twos.each do |flower_two| %>
  <tr>
    <td><%= flower_two.latin_name %></td>
    <td><%= flower_two.common_name %></td>
    <td><%= flower_two.bed_id %></td>
    <td><%= link_to 'Show', flower_two %></td>
    <td><%= link_to 'Edit', edit_flower_two_path(flower_two) %></td>
